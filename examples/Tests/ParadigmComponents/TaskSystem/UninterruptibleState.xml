<?xml version="1.0" standalone="no"?>
<monkeyml version="1.0">
    <io_devices tag="IO Devices"></io_devices>
    <variables tag="Variables"></variables>
    <sounds tag="Sounds"></sounds>
    <stimuli tag="Stimuli"></stimuli>
    <filters tag="Filters"></filters>
    <optimizers tag="Optimizers"></optimizers>
    <experiment tag="New Experiment" full_name="" description="">
        <protocol tag="New Protocol" nsamples="1" sampling_method="cycles" selection="sequential" interruptible="YES">
            <task_system tag="New Task System" interruptible="YES">
                <task_system_state tag="Begin State System" interruptible="NO">
                    <action_marker _unmoveable="1" tag="Actions"></action_marker>
                    <action type="report" tag="Report" message="Experiment started. Waiting."></action>
                    <action type="wait" tag="Wait" duration="3" duration_units="s"></action>
                    <action type="report" tag="Report 2" message="Stopping experiment"></action>
                    <action tag="Stop Experiment" type="stop_experiment"></action>
                    <action type="report" tag="Report 3" message="Exiting uninterruptible state"></action>
                    <transition_marker _unmoveable="1" tag="Transitions"></transition_marker>
                    <transition tag="Go to Exit" type="direct" target="Exit State System"></transition>
                </task_system_state>
                <task_system_state tag="Exit State System" interruptible="YES">
                    <action_marker _unmoveable="1" tag="Actions"></action_marker>
                    <action type="assert" tag="Assert" condition="0" message="Experiment didn't stop!" stop_on_failure="0"></action>
                    <transition_marker _unmoveable="1" tag="Transitions"></transition_marker>
                    <transition tag="Return to parent task system" type="yield"></transition>
                </task_system_state>
            </task_system>
        </protocol>
    </experiment>
</monkeyml>