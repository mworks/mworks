frame_list dynamic_noise (
    stimulus_group = dynamic_noise_frames
    loop = true
    autoplay = true
    )

stimulus_group dynamic_noise_frames {
    white_noise_background (
        randomize_on_draw = true
        )
}

image_file img (
    path = 'flamingos.jpg'
    x_size = 20
    y_size = 20
    alpha_multiplier = 0.5
)


protocol {
    queue_stimulus (dynamic_noise)
    queue_stimulus (img)
    update_display ()
    wait (10s)
}
